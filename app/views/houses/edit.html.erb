<div class="row">
  <div class="col-md-2">
    
  </div>
  
  <div class="col-md-8">
    <h1>Edit House</h1>
    
    <%= form_for @house, :html => {:multipart => true} do |f| %>
      
      <%= f.label :photo %> <br />
      <%= f.file_field :photo %><br />
      
      <%= f.label :numberOfBedRooms %> <br />
      <%= f.text_field :numberOfBedRooms %><br />
      
      <%= f.label :numberOfBathRooms %> <br />
      <%= f.text_field :numberOfBathRooms %><br />
      
      <%= f.label :numberOfReceptionRooms %> <br />
      <%= f.text_field :numberOfReceptionRooms %><br />
      
      <%= f.label :size %> <br />
      <%= f.text_field :size %><br />
      
      <%= f.label :security %> <br />
      <%= f.text_field :security %><br />
      <br />
      
      <%= f.submit "Update", :class=>"btn btn-primary" %>
      
    <% end %>
    <%= link_to 'Back', houses_path %>
  </div>
  
  <div class="col-md-2">
    
  </div>
</div>